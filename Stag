class UndoRedoStack:
    def __init__(self):
        self.undo_Stack = []
        self.Redo_Stack = []

    def do_action(self, action):
        self.undo_Stack.append(action)
        self.Redo_stack.clear

    def undo(self):
        abc = self.undo_Stack.pop()
        self.Redo_Stack.append(abc)

    def redo(self):
        abc = self.Redo_Stack.pop()
        self.undo_Stack.append(abc)
